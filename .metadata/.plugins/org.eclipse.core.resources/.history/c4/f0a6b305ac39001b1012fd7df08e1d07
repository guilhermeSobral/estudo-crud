package com.crudcompleto.service;

import java.util.function.Function;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;
import org.springframework.stereotype.Service;

import com.crudcompleto.entities.Produto;
import com.crudcompleto.repositories.ProdutoRepository;

//@Service
@Configuration
public class ProdutoService implements CrudService {
	
	@Autowired
	private ProdutoRepository repository;
	
	@Bean
	public void a() {
		Function<ProdutoRepository, Produto> function = repository -> repository.findById(1L).get();
		System.out.println(teste(function, repository));
	}
	
	
}
